<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="461.0" prefWidth="625.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.achordion.client.ui.StartPageController">
   <Label fx:id="AnyText" prefHeight="263.0" prefWidth="503.0" style="-fx-font-size: 26;" text="Welcome to Achordion!" textAlignment="CENTER" />
   <HBox alignment="CENTER" spacing="10.0">
      <Button mnemonicParsing="false" onMouseClicked="#onOfflineClicked" text="Offline" />
      <TextField fx:id="textField" onKeyPressed="#keyPressed" prefHeight="25.0" prefWidth="150.0" promptText="Enter Server ID" />
      <Button mnemonicParsing="false" onAction="#onClickToConnectClicked" onMouseClicked="#onClickToConnectClicked" text="Click To Connect" />
   </HBox>
</VBox>
