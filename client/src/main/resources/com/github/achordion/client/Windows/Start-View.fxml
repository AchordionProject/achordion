<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" onMouseClicked="#onOfflineClicked" prefHeight="379.0" prefWidth="593.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.achordion.client.ui.StartPageController">
   <Label fx:id="AnyText" prefHeight="19.0" prefWidth="303.0" style="-fx-font-size: 26;" text="Welcome to Achordion!" textAlignment="CENTER" />
   <HBox alignment="CENTER" spacing="10.0">
      <Button mnemonicParsing="false" text="Offline" />
      <TextField fx:id="textField" onKeyPressed="#keyPressed" prefHeight="25.0" prefWidth="150.0" promptText="Enter Server ID" />
      <Button mnemonicParsing="false" onAction="#onClickToConnectClicked" text="Click To Connect" />
   </HBox>
</VBox>
